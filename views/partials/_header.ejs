        <a href="/urls/new"><button type="button" class="btn btn-success">Create New URL</button></a>
      </div>
    </div>
        <% if (!user) { %> 
          <ul style=" float: right;" class="nav nav-link">
          <a href="/login"><button type="button" class="btn btn-success">Login</button></a>
          </ul>
          <a href="/register"><button type="button" class="btn btn-success">Register</button></a>
        <% } %> 
        <% if (user) { %>
         <p>Logged in as: <%=user.email%></p>
            <form class="form-inline" method="post" action="/logout">
            <button class="btn btn-outline-danger my-sm-0">
             Logout</button>
